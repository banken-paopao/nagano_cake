  <div class="field-group  form-group">
    <%= f.label :postal_code, "郵便番号（ハイフンなし）", class: "col-4" %>
    <%= f.text_field :postal_code, placeholder: "1234567", class: "col-4 form-control d-inline-block col-4" %>
  </div>
  <div class="field-group form-group mt-1">
    <%= f.label :address,"住所", class: "col-4" %>
    <%= f.text_field :address, placeholder: "東京都渋谷区代々木神園町0-0", class: "col-6 form-control d-inline-block" %>
  </div>
  <div class="field-group form-group  mt-1">
    <%= f.label :name, "宛名", class: "col-4" %>
    <%= f.text_field :name, placeholder: "令和通子", class: "col-4 form-control d-inline-block" %>
    <%= f.submit text, class: "btn btn-template-reverse ml-5 my-auto" %>
  </div>
